class File {
    let path = ""
    let handle = 0

    func open(p, mode) {
        path = p
        handle = native_call("fs_open", path, mode)
    }

    func write(content) {
        native_call("fs_write", handle, content)
    }

    func close() {
        native_call("fs_close", handle)
    }
}
